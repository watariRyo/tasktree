create_table "day_of_week", primary_key: "id", id: {type: :integer, comment:"ID"}, charset: "utf8mb4", collation: "utf8mb4_general_ci", comment: "曜日マスタ", force: :cascade do |t|
  t.integer "day_of_week", null: false, comment: "曜日"
  t.datetime "created_at", null: false, comment: "作成日時"
  t.datetime "updated_at", null: false, comment: "更新日時"
end
